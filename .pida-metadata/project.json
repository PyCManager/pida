{
  "targets":[
    {
      "name":"debug",
      "actions":[
        {
          "type":"shell",
          "options":{
            "cwd":null
          },
          "value":"python -u ./run-pida.py -D -w testing"
        }
      ]
    },
    {
      "name":"run",
      "actions":[
        {
          "type":"shell",
          "options":{},
          "value":"./run-pida.py"
        }
      ]
    },
    {
      "name":"genapi",
      "actions":[
        {
          "type":"shell",
          "options":{},
          "value":"pydoctor -c tools/pida-pydoctor.cfg"
        }
      ]
    },
    {
      "name":"tests",
      "actions":[
        {
          "type":"shell",
          "options":{},
          "value":"nosetests"
        }
      ]
    },
    {
      "name":"build.ext",
      "actions":[
        {
          "type":"shell",
          "options":{},
          "value":"python setup.py build_ext -i"
        }
      ]
    },
    {
      "name":"clean",
      "actions":[
        {
          "type":"shell",
          "options":{},
          "value":"python setup.py clean"
        },
        {
          "type":"shell",
          "options":{},
          "value":"make -C contrib/moo clean"
        }
      ]
    },
    {
      "name":"pyshell",
      "actions":[
        {
          "type":"shell",
          "options":{},
          "value":"./run-pida-pyshell.py --ipython"
        }
      ]
    },
    {
      "name":"gdb",
      "actions":[
        {
          "type":"shell",
          "options":{},
          "value":"gdb --args python -u run-pida.py -w testing -D"
        }
      ]
    }
  ],
  "options":{}
}