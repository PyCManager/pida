<?xml version="1.0" encoding="UTF-8"?>
<glade-catalog name="kiwiwidgets" library="kiwiwidgets" domain="glade-3" depends="gtk+" language="python">
  <glade-widget-classes>
    <glade-widget-class title="HyperLink" name="HyperLink" generic-name="hyperlink"/>
    <glade-widget-class title="ObjectList" name="ObjectList" generic-name="objectlist"/>
    <glade-widget-class title="ObjectTree" name="ObjectTree" generic-name="objecttree"/>
    <glade-widget-class title="KiwiLabel" name="ProxyLabel" generic-name="kiwilabel"/>
    <glade-widget-class title="KiwiComboBox" name="ProxyComboBox" generic-name="kiwicombobox"/>
    <glade-widget-class title="KiwiComboBoxEntry" name="ProxyComboBoxEntry" generic-name="kiwicomboboxentry"/>
    <glade-widget-class title="KiwiCheckButton" name="ProxyCheckButton" generic-name="kiwicheckbutton"/>
    <glade-widget-class title="KiwiRadioButton" name="ProxyRadioButton" generic-name="kiwiradiobutton"/>
    <glade-widget-class title="KiwiEntry" name="ProxyEntry" generic-name="kiwientry"/>

    <!-- DateEntry -->
    <glade-widget-class title="KiwiDateEntry"
                        name="ProxyDateEntry"
                        generic-name="kiwidateentry">
        <properties>
            <!-- Disable the size query box -->
            <property id="size"
                      query="False"
                      disabled="True"
                      save="False" />
        </properties>
    </glade-widget-class>

    <glade-widget-class title="KiwiSpinButton" name="ProxySpinButton" generic-name="kiwispinbutton"/>
    <glade-widget-class title="KiwiTextView" name="ProxyTextView" generic-name="kiwitextview"/>
    <glade-widget-class title="KiwiButton" name="ProxyButton" generic-name="kiwibutton"/>
  </glade-widget-classes>
  <glade-widget-group name="KiwiWidgets" title="Kiwi Widgets">
    <glade-widget-class-ref name="HyperLink"/>
    <glade-widget-class-ref name="ObjectList"/>
    <glade-widget-class-ref name="ObjectTree"/>
    <glade-widget-class-ref name="ProxyLabel"/>
    <glade-widget-class-ref name="ProxyComboBox"/>
    <glade-widget-class-ref name="ProxyComboBoxEntry"/>
    <glade-widget-class-ref name="ProxyCheckButton"/>
    <glade-widget-class-ref name="ProxyRadioButton"/>
    <glade-widget-class-ref name="ProxyEntry"/>
    <glade-widget-class-ref name="ProxyDateEntry"/>
    <glade-widget-class-ref name="ProxySpinButton"/>
    <glade-widget-class-ref name="ProxyTextView"/>
    <glade-widget-class-ref name="ProxyButton"/>
  </glade-widget-group>
</glade-catalog>

