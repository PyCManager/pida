name = pida-next
[Run PIDA]
controller = PYTHON_CONTROLLER
default = True
execute_file = run-pida.py
[Generate API Documentation]
controller = GENERIC_EXECUTION
default = ""
command = pydoctor -c tools/pida-pydoctor.cfg
[Run Tests]
controller = GENERIC_EXECUTION
default = ""
command = nosetests tests/
[Debug PIDA]
controller = GENERIC_DEBUGGER
default = ""
debugger = pydb
executable = bin/pida
parameters = ""
[[breakpoint]]
[[prebreakpoint]]
