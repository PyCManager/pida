name = pida-next
[Execute PIDA]
controller = PYTHON_CONTROLLER
default = True
execute_file = pida.py
[Run PIDA Tests]
controller = GENERIC_EXECUTION
default = ""
command = nosetests tests/
[Build API Documentation]
controller = GENERIC_EXECUTION
default = ""
command = pydoctor -c tools/pida-pydoctor.cfg
[Build PIDA]
controller = DISTUTILS_CONTROLLER
default = ""
command = build