<?xml version="1.0"?>
<!--*- mode: xml -*-->
<interface>
  <object class="GtkUIManager" id="uimanager">
    <child>
      <object class="GtkActionGroup" id="test_ag">
        <child>
          <object class="GtkAction" id="test_act">
            <property name="name">test_act</property>
            <property name="label" translatable="yes">test action</property>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkWindow" id="test_view">
    <property name="default_height">250</property>
    <property name="default_width">440</property>
    <child>
      <object class="GtkVBox" id="vb">
        <property name="visible">True</property>
        <child>
          <object class="GtkHBox" id="hbox1">
            <property name="visible">True</property>
            <child>
              <object class="GtkButton" id="b1">
                <property name="is_focus">True</property>
                <property context="yes" name="label" translatable="yes">button1</property>
                <property name="visible">True</property>
              </object>
            </child>
            <child>
              <placeholder/>
            </child>
            <child>
              <placeholder/>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="label1">
            <property context="yes" name="label" translatable="yes">I AM A TEST</property>
            <property name="visible">True</property>
            <property name="xalign">0.0</property>
          </object>
          <packing>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <placeholder/>
        </child>
      </object>
    </child>
  </object>
</interface>
